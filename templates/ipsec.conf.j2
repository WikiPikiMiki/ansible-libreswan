# DO NOT EDIT MANUALLY
config setup
       dumpdir=/var/run/pluto/
       protostack={% if protostack is defined %}{{ protostack }}{% endif %}
       plutodebug=all
       plutostderrlog={% if plutostderrlog is defined %}{{ plutostderrlog }}{% else %}/var/log/pluto.log
       {% endif %}

       {% if setup_items is defined %}
       {{ setup_items }}
       {% endif %}
       
{% if tunnel_configuration is defined %}
{% for t in tunnel_configuration %}
# /etc/ipsec.conf - Libreswan IPsec configuration file
conn {{ t.conn_name }}
     {{ t.conn_configuration }}
{% else %}# /etc/ipsec.conf - Libreswan IPsec configuration file
{% endfor %}{% endif %}include /etc/ipsec.d/*.conf